<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" >

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label=""
        android:supportsRtl="true"
        android:theme="@style/Theme.OurApplication"
        android:windowSoftInputMode="adjustPan"
        tools:targetApi="31">
        <activity
            android:name=".MainActivity"
            android:exported="false"></activity>
        <activity
            android:name=".MainUnityActivity"
            android:exported="true"></activity>
        <activity
            android:name=".SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ChatMainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TutorialActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SignInActivity">
        </activity>
        <activity
            android:name=".RecommendActivity">
        </activity>
        <activity
            android:name=".TestActivity">
        </activity>
        <activity
            android:name=".DiaryMainActivity">
        </activity>
        <activity
            android:name=".DiaryWriteActivity">
        </activity>
        <activity
            android:name=".DiaryPhotoActivity">
        </activity>
        <activity
            android:name=".DiarySelectKeywordActivity">
        </activity>
        <activity
            android:name=".DiaryProcessActivity">
        </activity>
        <activity
            android:name=".TodoMainActivity">
        </activity>
        <receiver
            android:name="com.hello.ourApplication.TodoAlarmReceiver"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
                <action android:name="android.intent.action.LOCKED_BOOT_COMPLETED" />
            </intent-filter>
        </receiver>
    </application>

</manifest>